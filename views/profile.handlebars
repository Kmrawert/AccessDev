<div id="formContainer">
    <div id="formArea">
        <form action="/profile">
        <label for="Name">Name:</label>
            <input type="text" name="fname"><br>
        <label for="proImage">Profile Image:</label>
        
        <label for="location">Location:</label>
            <input type="text" name="location"><br>
        <label for="briefBio">Brief Bio:</label>
           <textarea name="briefBio" id="briefBio"></textarea>
        <label for="YTLinks">YouTube Link:</label>
           <textarea name="YTLinks" id="YTLinks"></textarea>
        <label for="start">Instrument:</label>
            <input type="text" id="instrument" name="instrumName">
        <div>
        <input type="radio" id="Keys" name="instrument" value="Keys"
         checked>
        <label for="Keys">keys</label>
        <input type="radio" id="Bass" name="instrument" value="Bass"
         checked>
        <label for="Bass">Bass</label>
        <input type="radio" id="Guitar" name="instrument" value="Guitar"
         checked>
        <label for="Guitar">Guitar</label>
        </div>
        <label for="moneyEarned">Money Earned to Date</label>
            <input type="text" name="moneyEarnedtoDate"><br>
        
        <input type="submit" value="Submit">

        </form>
    </div>
</div>